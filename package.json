{
    "name": "ripe-sdk",
    "version": "1.4.10",
    "description": "The public SDK for RIPE Core",
    "keywords": [
        "js",
        "ripe",
        "sdk"
    ],
    "homepage": "https://github.com/ripe-tech/ripe-sdk#readme",
    "bugs": {
        "url": "https://github.com/ripe-tech/ripe-sdk/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ripe-tech/ripe-sdk.git"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Platforme",
        "url": "https://www.platforme.com"
    },
    "files": [
        "gulpfile.js",
        "dist/**/*",
        "res/**/*",
        "src/css/**/*.css",
        "src/js/index.js",
        "src/js/ripe.js",
        "src/js/ripe-min.js",
        "src/js/api/**/*.js",
        "src/js/base/**/*.js",
        "src/js/plugins/**/*.js",
        "src/js/visual/**/*.js",
        "test/**/*.js"
    ],
    "main": "src/js/index.js",
    "scripts": {
        "build": "gulp build",
        "docs": "gulp docs",
        "gulp": "gulp",
        "lint": "gulp lint",
        "mark": "gulp mark",
        "test": "gulp test",
        "prettier": "prettier \"./*.{js,json}\" \"./{src,test}/**/*.{js,json}\" --write",
        "upgrade": "npx sort-package-json && ncu -au",
        "watch": "gulp watch"
    },
    "dependencies": {
        "xmlhttprequest": "^1.8.0"
    },
    "devDependencies": {
        "eslint": "^5.12.0",
        "eslint-config-hive": "^0.1.7",
        "gulp": "^4.0.0",
        "gulp-cli": "^2.0.1",
        "gulp-count": "^1.0.0",
        "gulp-eslint": "^5.0.0",
        "gulp-jsdoc3": "^2.0.0",
        "gulp-mocha": "^6.0.0",
        "gulp-replace": "^1.0.0",
        "gulp-size": "^3.0.0",
        "gulp-uglifyes": "^0.2.1",
        "gulp-zip": "^4.2.0",
        "jsdom": "^13.1.0",
        "mocha": "^5.2.0",
        "mocha-cli": "^1.0.1",
        "npm-check-updates": "^2.15.0",
        "prettier": "^1.15.3",
        "prettier-config-hive": "^0.1.1",
        "sort-package-json": "^1.17.1"
    },
    "unpkg": "dist/ripe.js"
}
